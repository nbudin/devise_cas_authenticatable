                },
                error: function(err, status, xhr){
                    //console.log(err, status, xhr);
                    return cb(err);
                },
                success: function(data, status, xhr){
                    return cb(null, data);
                }
            });
        };

        var checkIntervalHandle;

        var updateAppStatus = function() {
            issueAppCommand("status", function(err, data) {
                if (err) {
                    $message.html("We're experiencing some difficulty right now. Please try refreshing. If that doesn't work, shoot us an email at support@bushi.do and we'll get on it as soon as we can.");
                } else if ("stopped" === data.status) {
                    issueAppCommand("start", function() {});
                } else {
                    $message.html("Your app is currently <strong>" + data.status + "</strong>");
                    $source.html("This app was deployed from " + data.app.app.url);

                    if ("running" === data.status) {
                        window.location = window.location;
                        clearInterval(checkIntervalHandle);
                    }
                }
            });
        };

        updateAppStatus();
        checkIntervalHandle = setInterval(updateAppStatus, 1000);
    })();
  </script>
  
